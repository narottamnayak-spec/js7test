{
  "orderPreparation": {
    "parameters": {
      "student": {
        "type": "String",
        "default": "\"akash\""
      },
      "select": {
        "type": "Boolean",
        "default": "false"
      },
      "student_id": {
        "final": "111"
      }
    },
    "allowUndeclared": false
  },
  "instructions": [
    {
      "TYPE": "Execute.Named",
      "jobName": "firstjob",
      "label": "firstjob"
    },
    {
      "TYPE": "Execute.Named",
      "jobName": "second_job",
      "label": "second_job"
    },
    {
      "TYPE": "Execute.Named",
      "jobName": "third_job",
      "label": "third_job"
    },
    {
      "TYPE": "Execute.Named",
      "jobName": "nodejob",
      "defaultArguments": {
        "node": "\"i_am_node\""
      },
      "label": "nodejob"
    },
    {
      "TYPE": "Execute.Named",
      "jobName": "final_job",
      "label": "final_job"
    }
  ],
  "timeZone": "Asia/Kolkata",
  "jobs": {
    "firstjob": {
      "agentName": "mainAgent",
      "subagentClusterId": "round_R",
      "withSubagentClusterIdExpr": false,
      "executable": {
        "TYPE": "ShellScriptExecutable",
        "script": "echo \"name of student :\" %STUDENT%\necho \"availability :\" %select%",
        "env": {
          "STUDENT": "$student",
          "select": "$select"
        },
        "v1Compatible": false
      },
      "skipIfNoAdmissionForOrderDay": false,
      "killAtEndOfAdmissionPeriod": false,
      "parallelism": 1,
      "graceTimeout": 1,
      "failOnErrWritten": false,
      "warnOnErrWritten": false,
      "isNotRestartable": false
    },
    "second_job": {
      "agentName": "mainAgent",
      "subagentClusterId": "round_R",
      "withSubagentClusterIdExpr": false,
      "executable": {
        "TYPE": "ShellScriptExecutable",
        "script": "echo \"name of student :\" %ST%\necho \"availability :\" %select%\n\nset ST= narrotam\n\necho ST =%ST%>> %JS7_RETURN_VALUES%\n ",
        "env": {
          "ST": "$student",
          "select": "$select"
        },
        "v1Compatible": false
      },
      "skipIfNoAdmissionForOrderDay": false,
      "killAtEndOfAdmissionPeriod": false,
      "parallelism": 1,
      "graceTimeout": 1,
      "failOnErrWritten": false,
      "warnOnErrWritten": false,
      "isNotRestartable": false
    },
    "third_job": {
      "agentName": "mainAgent",
      "subagentClusterId": "round_R",
      "withSubagentClusterIdExpr": false,
      "executable": {
        "TYPE": "ShellScriptExecutable",
        "script": "echo %student%\n ",
        "env": {
          "student": "$ST"
        },
        "v1Compatible": false
      },
      "skipIfNoAdmissionForOrderDay": false,
      "killAtEndOfAdmissionPeriod": false,
      "parallelism": 1,
      "graceTimeout": 1,
      "failOnErrWritten": false,
      "warnOnErrWritten": false,
      "isNotRestartable": false
    },
    "nodejob": {
      "agentName": "mainAgent",
      "subagentClusterId": "round_R",
      "withSubagentClusterIdExpr": false,
      "executable": {
        "TYPE": "ShellScriptExecutable",
        "script": "\necho %node%\n ",
        "env": {
          "node": "$node"
        },
        "v1Compatible": false
      },
      "skipIfNoAdmissionForOrderDay": false,
      "killAtEndOfAdmissionPeriod": false,
      "parallelism": 1,
      "graceTimeout": 1,
      "failOnErrWritten": false,
      "warnOnErrWritten": false,
      "isNotRestartable": false
    },
    "final_job": {
      "agentName": "mainAgent",
      "subagentClusterId": "round_R",
      "withSubagentClusterIdExpr": false,
      "executable": {
        "TYPE": "ShellScriptExecutable",
        "script": "\n echo \"stude id is : \"%student_id%",
        "env": {
          "student_id": "$student_id"
        },
        "v1Compatible": false
      },
      "skipIfNoAdmissionForOrderDay": false,
      "killAtEndOfAdmissionPeriod": false,
      "parallelism": 1,
      "graceTimeout": 1,
      "failOnErrWritten": false,
      "warnOnErrWritten": false,
      "isNotRestartable": false
    }
  }
}